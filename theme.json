{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        {
          "slug": "primary",
          "color": "#2c185c",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#00ff00",
          "name": "Secondary"
        },
        {
          "slug": "text",
          "color": "#222",
          "name": "Text Color"
        }
      ],
      "custom": false,
      "customGradient": false,
      "defaultPalette": false,
      "defaultGradients": false
    },
    "custom": {
      "spacing": {},
      "typography": {
        "font-size": {},
        "line-height": {}
      }
    },
    "spacing": {
      "padding": true,
      "units": ["px", "%", "em", "rem", "vw", "vh"]
    },
    "typography": {
      "fontSizes": [
        {
          "name": "Tiny",
          "size": "1.024rem",
          "slug": "tiny"
        },
        {
          "name": "Small",
          "size": "1.28rem",
          "slug": "small"
        },
        {
          "name": "Normal",
          "size": "1.6rem",
          "slug": "normal"
        },
        {
          "name": "Large",
          "size": "2rem",
          "slug": "large"
        },
        {
          "name": "Extra Large",
          "size": "2.5rem",
          "slug": "extra-large"
        },
        {
          "name": "Huge",
          "size": "3.2rem",
          "slug": "huge"
        },
        {
          "name": "Gigantic",
          "size": "3.9rem",
          "slug": "gigantic"
        }
      ]
    }
  }
}
